  <build>
  <finalName>agtsys</finalName>
  <pluginManagement>
  <plugins>
        <plugin>
          <groupId>org.apache.tomcat.maven</groupId>
          <artifactId>tomcat7-maven-plugin</artifactId>
          <version>2.2</version>
          <configuration>
          <path>/agtsys</path>
          <uriEncoding>UTF-8</uriEncoding>
          </configuration>
        </plugin>
      </plugins>
